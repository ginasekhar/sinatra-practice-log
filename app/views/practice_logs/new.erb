<header>
<h2>Create New Practice Log</h2>
</header>

<%= @error %>
  <form action='/practice_logs' method="post">
    <br>
    <label for="date"> Date: </label>
    <input type="date" name="date"><br>
    <br>
    <label for="practice_minutes"> Minutes Practiced: </label>
    <input type="text" name="practice_minutes"><br>
    <br>
    <label for="notes"> Notes (brief notes on what you worked on, what your next steps are): </label>
    <input type="text" name="notes"><br>
    <br>

    <input type="submit" value="submit" id="submit">

  </form>

